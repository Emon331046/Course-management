<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginValidateCheck</servlet-name>
        <servlet-class>cse.web.loginValidateCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doLogout</servlet-name>
        <servlet-class>cse.web.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>takecoursecheck</servlet-name>
        <servlet-class>cse.web.takecoursecheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addcourseServlet</servlet-name>
        <servlet-class>cse.web.addcourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewcourseServlet</servlet-name>
        <servlet-class>cse.web.viewcourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>cse.web.Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginValidateCheck</servlet-name>
        <url-pattern>/loginValidateCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doLogout</servlet-name>
        <url-pattern>/doLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>takecoursecheck</servlet-name>
        <url-pattern>/takecoursecheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addcourseServlet</servlet-name>
        <url-pattern>/addcourseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewcourseServlet</servlet-name>
        <url-pattern>/viewcourseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
